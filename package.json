{
  "name": "fancylog",
  "version": "4.0.2",
  "description": "Fancy Logging!",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "prepublishOnly": "yarn run test && yarn run build",
    "test": "yarn run check && yarn run lint",
    "check": "yarn run build --noEmit",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "prebuild": "rimraf lib",
    "build": "tsc",
    "build:dev": "yarn run build --sourceMap",
    "watch": "tsc --watch",
    "dev": "yarn run watch --sourceMap"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lolPants/fancylog.git"
  },
  "keywords": [
    "fancy",
    "log",
    "logging",
    "console",
    "file"
  ],
  "author": "Jack Baron <jackmbaron@gmail.com> (https://www.jackbaron.com)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lolPants/fancylog/issues"
  },
  "homepage": "https://github.com/lolPants/fancylog#readme",
  "devDependencies": {
    "@types/dateformat": "^3.0.0",
    "@types/mkdirp": "^0.5.2",
    "@types/node": "^11.9.3",
    "@types/strip-ansi": "^3.0.0",
    "prettier": "^1.16.4",
    "rimraf": "^2.6.3",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.3.3"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "dateformat": "^3.0.3",
    "mkdirp": "^0.5.1",
    "strip-ansi": "^5.0.0"
  }
}
